,MADA;AAEA,kBAAA,SAAA,EAAA,KAAA,CAAA,OAAA,CAAA,SAFA;AAGA,kBAAA,MAAA,EAAA,KAAA,CAAA,WAAA,CAAA,MAHA;AAIA,kBAAA,QAAA,EAAA;AAJA,iBAAA,CAFA;;AAAA;AAQA,gBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,KAAA,CAAA,WAAA,GAAA,SAAA,EARA,CASA;;;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,0BAAA;;AAVA;AAAA;;AAAA;AAAA;AAAA;;AAYA,gBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,OAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA;AA5BA;AARA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"6\">\r\n        <div class=\"product-img-wrapper\">\r\n          <el-image :src=\"product.productImg\" :preview-src-list=\"[product.productImg]\" fit=\"cover\" style=\"width:100%;height:100%\"></el-image>\r\n        </div>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <h2>{{product.productName}}</h2>\r\n        <p class=\"product-detail\">{{product.productDesc}}</p>\r\n        <p class=\"product-sales\">月售:{{product.sales}}份 好评率:{{product.positiveRate}}%</p>\r\n        <el-radio-group\r\n        size=\"mini\"\r\n        v-model=\"choosedSpec\"\r\n        >\r\n          <el-radio-button\r\n          :label=\"item1.specName\"\r\n          v-for=\"item1 in product.productSpecList\"\r\n          :key=\"item1.specId\"\r\n          @click.native=\"handleSpecClick(item1)\"\r\n          />\r\n        </el-radio-group>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <h1 class=\"product-price\" v-if=\"!showPrice()\">￥{{getPriceRange(product)}}</h1>\r\n        <h1 class=\"product-price\" v-if=\"showPrice()\">\r\n          ￥{{currentSpec.price}}\r\n          <p style=\"font-size:14px;color:#bbb\">包装费:￥ {{currentSpec.packageFee}}</p>\r\n        </h1>\r\n        <el-button type=\"primary\" icon=\"el-icon-plus\" :disabled=\"!showPrice()\" circle size=\"mini\" @click=\"intoCart\"></el-button>\r\n      </el-col>\r\n    </el-row>\r\n    <el-divider></el-divider>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport productService from '../../service/ProductService'\r\nimport cartService from '../../service/CartService'\r\nexport default {\r\n  props: ['product', 'shopId'],\r\n  data () {\r\n    return {\r\n      choosedSpec: '',\r\n      currentSpec: {}\r\n    }\r\n  },\r\n  methods: {\r\n    showPrice () {\r\n      return this.choosedSpec\r\n    },\r\n    getPriceRange (product) {\r\n      const price = productService.getProductPriceRange(product)\r\n      if (price.lowPrice === price.highPrice) {\r\n        return price.lowPrice\r\n      }\r\n      return price.lowPrice + '-' + price.highPrice\r\n    },\r\n    handleSpecClick (item1) {\r\n      this.currentSpec = item1\r\n    },\r\n    async intoCart () {\r\n      try {\r\n        await cartService.addProduct({\r\n          shopId: this.shopId,\r\n          productId: this.product.productId,\r\n          specId: this.currentSpec.specId,\r\n          quantity: 1\r\n        })\r\n        this.$message.success(this.choosedSpec + '加入购物车成功')\r\n        // 触发购物车更新\r\n        this.$store.commit('updateLastProductAddTime')\r\n      } catch (e) {\r\n        this.$message.error(e.message)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .product-img-wrapper {\r\n      width:200px;\r\n      height:128px;\r\n      .el-image {\r\n        border-radius: 5px;\r\n      }\r\n    }\r\n    li :hover {\r\n      cursor: pointer;\r\n    }\r\n    h2 {\r\n      font-size: 18px;\r\n      margin: 0;\r\n    }\r\n    .product-detail {\r\n      font-size: 14px;\r\n      color: #666;\r\n    }\r\n    .product-sales {\r\n      font-size: 14px;\r\n    }\r\n    .product-price {\r\n      margin: 0;\r\n      margin-bottom: 40px;\r\n      font-size: 24px;\r\n      color: #f60;\r\n    }\r\n    .el-button {\r\n      margin-left: 30px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/shop"}]}